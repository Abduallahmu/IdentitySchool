<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>School Al Mouallem</title>

    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.0/css/bootstrap.min.css">
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.3.1/jquery.min.js"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.4.0/js/bootstrap.min.js"></script>
    <link href="~/css/Style.css" rel="stylesheet" />
    <script src="https://ajax.aspnetcdn.com/ajax/jQuery/jquery-3.3.1.min.js"></script>
</head>
<body>
    <header>
        <nav class="nav navbar-collapse">
            <div class="container-fluid">
                <div class="navbar-header">
                    <a class="navbar-brand" asp-area="" asp-controller="Home" asp-action="Index" id="Almouallem">School Al Mouallem IT</a>
                </div>
                <ul class="nav navbar-nav">

                    <li class="nav-item" id="NavBar">
                        <a class="nav-link text-dark" asp-area="" asp-controller="Teacher" asp-action="Index">Teachers</a>
                    </li>
                    <li class="nav-item" id="NavBar">
                        <a class="nav-link text-dark" asp-area="" asp-controller="Student" asp-action="Index">Students</a>
                    </li>
                    <li class="nav-item" id="NavBar">
                        <a class="nav-link text-dark" asp-area="" asp-controller="Course" asp-action="Index">Courses</a>
                    </li>
                    <li class="nav-item" id="NavBar">@Html.ActionLink("About", "About", "Home")</li>
                    <li class="nav-item" id="NavBar">@Html.ActionLink("Contact", "Contact", "Home")</li>
                </ul>
            </div>
        </nav>        
    </header>
    <div>
        @RenderBody()
    </div>
    <footer id="footerr">
        <p>Abdullah Mouallem &copy; @DateTime.Now.ToString("yyyy")</p>
    </footer>
</body>
</html>
